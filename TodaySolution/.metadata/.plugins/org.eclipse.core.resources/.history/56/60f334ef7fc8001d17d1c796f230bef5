package boj;

import java.util.Scanner;

public class boj4779 {
	static int idx;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		StringBuilder sb = new StringBuilder();
		
		while(true) {
			//N 입력
			int N = sc.nextInt();
			//크기가 3^N인 배열 "-"로 채우기
			char[] jakdaegi = new char[(int)Math.pow(3, N)];
			for(int i=0; i<jakdaegi.length; i++) {
				jakdaegi[i] = '-';
			}
		}
		
	}
	
	public static String kanto(String[] arr, int st, int ed) {
		int mid1 = (ed-st+1)/3 - 1;
		int mid2 = (2*(ed-st+1))/3 - 1;
		
		if(arr.length==1) {
			return "-";
		}
		
		if(idx>mid1 && idx<=mid2) {
			return arr[idx] = null;
		}else if(idx<=mid1) {
			return kanto(arr, st, mid1);
		}else if(idx>mid2) {
			return kanto(arr, mid2, ed);
		}
		return "-";
		
		
	}
}
